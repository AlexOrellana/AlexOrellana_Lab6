#pragma once
#include<string>
#include<vector>

using std::string;
using std::vector;

class Usuario{
	string username;
	string password;
	vector<int>games;
public:
	Usuario(string,string);
	
	string getUsername();
	string getPassword();
	
	string toString()const;
	
	void setPassword(string);

	void addGame(int id);
	void deleteGame(int id);
	void imprimirGames();	
};
